<?xml version="1.0" encoding="UTF-8"?>
<EmbeddedServiceConfig xmlns="http://soap.sforce.com/2006/04/metadata">
    <areGuestUsersAllowed>false</areGuestUsersAllowed>
    <deploymentFeature>FieldService</deploymentFeature>
    <deploymentType>Web</deploymentType>
    <embeddedServiceAppointmentSettings>
        <enabled>true</enabled>
        <shouldShowExistingAppointment>true</shouldShowExistingAppointment>
        <shouldShowNewAppointment>true</shouldShowNewAppointment>
    </embeddedServiceAppointmentSettings>
    <embeddedServiceFlows>
        <flow>runtime_appointmentbooking__In_New</flow>
        <flowType>FS_NewAppointment</flowType>
        <isAuthenticationRequired>true</isAuthenticationRequired>
    </embeddedServiceFlows>
    <embeddedServiceFlows>
        <flow>runtime_appointmentbooking__In_Modify</flow>
        <flowType>FS_ModifyAppointment</flowType>
        <isAuthenticationRequired>true</isAuthenticationRequired>
    </embeddedServiceFlows>
    <embeddedServiceFlows>
        <flow>runtime_appointmentbooking__In_Cancel</flow>
        <flowType>FS_CancelAppointment</flowType>
        <isAuthenticationRequired>true</isAuthenticationRequired>
    </embeddedServiceFlows>
    <isEnabled>true</isEnabled>
    <masterLabel>Lightning Scheduler Inbound Scheduling</masterLabel>
    <shouldHideAuthDialog>false</shouldHideAuthDialog>
    <site>Professional_Community</site>
</EmbeddedServiceConfig>
